			interrupt-map = <0x00 0x00 0x00 0x00 0x3f 0x17 0x04>;
			interrupts = <0x17 0x04>;
			num-lanes = <0x01>;
			clock-names = "core\0reg";
			clocks = <0x40 0x01 0x0c 0x40 0x01 0x0e>;
			status = "okay";
			num-viewport = <0x08>;
			phys = <0x52 0x02>;
		};
	};
};

&cp0_uart0 {
    status = "okay";
    mcu {
        compatible = "qnap,ts435xeu-mcu";

        pwm: pwm-0 {
            #cooling-cells = <2>;
