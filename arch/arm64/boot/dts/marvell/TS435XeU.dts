					phandle = <0x1b>;
				};
			};

			edac-mc@20000 {
				compatible = "marvell,armada-ap80x-edac-mc";
				reg = <0x20000 0x1000>;
				interrupts = <0x00 0x05 0x04>;
				status = "okay";
			};

			musdk_cma {
				compatible = "marvell,musdk-cma";
				dma-coherent;
			};
		};
